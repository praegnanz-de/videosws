{
  "name": "leanback",
  "version": "0.8.0.93",
  "url": "http://www.leanbackplayer.com",
  "description": "A main feature of LeanBack Player in desktop browsers is to be controlled almost completely by keyboard. The controls are similar to keyboard shortcuts known from widely-used desktop media players.",
  "repository": "http://github.com/johndyer/mediaelement/",
  "license": "gnugpl",
  "pricing": {
    "once": true,
    "subscription": false,
    "freeAvailable": true
  },
  "library": [],
  "hosted": false,

  "example": {
    "css": ["css.player/leanbackPlayer.default.css"],
    "jsHead": [
      "js.player/leanbackPlayer.pack.js", 
      "js.player/leanbackPlayer.en.js", 
      "js.player/leanbackPlayer.de.js"]
  },

  "flags": {
    "flash": true,
    "api": true,
    "unifiedLook": true,
    "unifiedAPI": true,
    "fullscreen": true,
    "keyboard": true,
    "subtitles": true,
    "chapters": true,
    "playlists": true,
    "responsive": false,
    "embeddable": false,
    "cms": ["wordpress", "joomla"],
    "skinnable": true,
    "audioOnly": true,
    "speedControl": false,
    "qualityControl": true,
    "aria": false,
    "hls": false,
    "dash": false
  }
}