{
  "name": "mediaelement.js",
  "version": "3.0.0",
  "url": "http://mediaelementjs.com/",
  "description": "Our personal favourite. Sports the right balance between mighty API and simple usage. Very clever Flash fallback with unified API.",
  "repository": "http://github.com/johndyer/mediaelement/",
  "license": "mit",
  "pricing": {
    "once": false,
    "subscription": false,
    "freeAvailable": true
  },
  "library": ["jquery"],
  "hosted": "possible",

  "example": {
    "css": ["mediaelementplayer.css"],
    "jsHead": ["jquery.js", "mediaelement-and-player.min.js"]
  },

  "flags": {
    "flash": true,
    "api": true,
    "unifiedLook": true,
    "unifiedAPI": true,
    "fullscreen": true,
    "keyboard": true,
    "subtitles": true,
    "chapters": true,
    "playlists": false,
    "responsive": true,
    "embeddable": false,
    "cms": ["wordpress", "drupal", "joomla", "typo3", "contao"],
    "services": ["youtube", "vimeo", "dailymotion"],
    "skinnable": true,
    "audioOnly": true,
    "speedControl": false,
    "qualityControl": false,
    "aria": true,
    "hls": true,
    "dash": true
  }
}